{
  "name": "image-generation-mcp",
  "version": "1.1.1",
  "description": "MCP server for generating blog images using AI (Nano Banana/Gemini and more)",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "image-generation-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check": "npm run typecheck && npm run lint && npm run format:check && npm run test"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "image-generation",
    "gemini",
    "nano-banana",
    "blog",
    "ghost",
    "ai"
  ],
  "author": "humanwritten",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "dotenv": "^16.4.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.4.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/12-days-of-shipmas-2025/day-2-image-generation-mcp.git"
  },
  "homepage": "https://github.com/12-days-of-shipmas-2025/day-2-image-generation-mcp#readme",
  "bugs": {
    "url": "https://github.com/12-days-of-shipmas-2025/day-2-image-generation-mcp/issues"
  }
}
